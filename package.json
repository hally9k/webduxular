{
  "name": "webduxular",
  "version": "0.0.1",
  "description": "A boilerplate project for AngularJS 1.5, Webpack, es2015, Babel, Redux with Bootstrap & Kendo UI.",
  "main": "index.js",
  "author": "hally9k",
  "license": "ISC",
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "start": "npm-run-all --parallel validate server:dev test:dev",
    "server:dev": "webpack-dev-server -d --devtool source-map --env.dev --content-base build/ --inline --hot --progress",
    "server:prod": "webpack-dev-server --env.prod --content-base build/ --inline --hot --progress",
    "lint": "eslint .",
    "validate": "npm-run-all --parallel validate-webpack:* lint test",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "build:dev": "npm run validate-webpack:dev && webpack -d --devtool source-map --env.dev --progress",
    "build:prod": "rimraf build && npm run validate-webpack:prod && webpack --env.prod --progress",
    "test": "karma start",
    "test:dev": "npm test -- --auto-watch --no-single-run --debug --browsers Chrome"
  },
  "devDependencies": {
    "angular": "^1.5.6",
    "angular-ui-router": "^0.3.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-stage-2": "^6.5.0",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.13.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-angular": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.6",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "0.0.4",
    "ghooks": "^1.2.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "immutable": "^3.8.1",
    "jquery": "2.2.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "kendo": "git+https://bower.telerik.com/npm-kendo-ui.git",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.0",
    "ng-redux": "^3.3.4-beta.0",
    "ngmin-webpack-plugin": "^0.1.3",
    "npm-run-all": "^2.2.0",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-ui-router": "^0.6.0",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "uglify-loader": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-validator": "^2.1.3"
  }
}
